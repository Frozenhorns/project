#include <stdlib.h>

#include "image.h"

t_image		*perlin_texture(t_color *begin, t_color *end)
{
  t_perlin	*perlin;
  t_image	*texture;

  if ((perlin = malloc(sizeof(*perlin))) == NULL)
    return (NULL);
  if ((texture = malloc(sizeof(*texture))) == NULL)
    return (NULL);
  return (texture);
}
